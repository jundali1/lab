#include<stdio.h>
#include<stdlib.h>
int minn(char *DNA1,char *DNA2,int EXCEL[10][10],int i,int j)
{
	int tmp1=EXCEL[i-1][j-1];
	int tmp2=EXCEL[i-1][j];
	int tmp3=EXCEL[i][j-1];
	int tmp11,tmp22,tmp33;
	if(DNA1[j-1]-'0'==DNA2[i-1]-'0') tmp11=tmp1+2;
	else tmp11=tmp1-2;
	tmp22=tmp2-3;
	tmp33=tmp3-3;
	int tmp=(tmp11>tmp22)?tmp11:tmp22;
	if(tmp33>tmp) tmp=tmp33;
    //printf("%d\n",tmp);
	return tmp;
}
int main(){
	int EXCEL[10][10];
	EXCEL[0][0]=0;
	char * DNA1 = "AGGGCT";
	char * DNA2 = "AGGCA";
	int length1=6;
	int length2=5;
	int i=0;
	int j=0;
	int point;
	int tmp;
	for(i=1;i<=length1;i++){
		EXCEL[0][i]=EXCEL[0][i-1]-3;
	}
	for(i=1;i<=length2;i++){
		EXCEL[i][0]=EXCEL[i-1][0]-3;
	}
	for(i=1;i<=length2;i++){
		for(j=1;j<=length1;j++){
			tmp=minn(DNA1,DNA2,EXCEL,i,j);
			EXCEL[i][j]=tmp;
			printf("%d %d %d\n",i,j,tmp);
		}
	}
	point=EXCEL[length2][length1];
	printf("point=%d",point);
  return 0;
}
